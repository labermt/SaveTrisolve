<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24.0"
    android:viewportHeight="24.0">

    <group android:name="flip">

        <path
            android:name="mirror"
            android:pathData="M 12,3
            l 0, 1
            m 0, 5
            l 0, 1
            m 0, 5
            l 0, 1
            m 0, 5
            l 0, 1"
            android:strokeColor="@color/light_gray"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

        <path
            android:name="left"
            android:pathData="M 1,20
            l 8, 0
            m 0, 0
            l -8, -16
            m 0, 0
            l 0, 16"
            android:strokeColor="@color/light_gray"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

        <path
            android:name="right"
            android:pathData="M 23,20
            l -8, 0
            m 0, 0
            l 8, -16
            m 0, 0
            l 0, 16"
            android:strokeColor="@android:color/white"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

    </group>

</vector>
