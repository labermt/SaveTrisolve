    android:viewportWidth="24.0"
    android:viewportHeight="24.0">

    <group android:name="cartesian_move">

        <path
            android:name="x_axis"
            android:pathData="M 22,18
            l -21,0"
            android:strokeColor="@android:color/white"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

        <path
