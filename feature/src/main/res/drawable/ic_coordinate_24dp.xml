            android:pathData="M 22,18
            l -3,-3
            m 3,3
            l -3,3"
            android:strokeColor="@android:color/white"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

        <path
            android:name="y_move"
            android:pathData="M 6,2
            l -3,3
            m 3,-3
            l 3,3"
            android:strokeColor="@android:color/white"
            android:strokeLineCap="round"
            android:strokeWidth="2" />

